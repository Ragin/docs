import React from 'react';
import Grid from '@mui/material/Grid';
import Typography from '@mui/material/Typography';

import {MedicationsTable} from '../medications-table/MedicationsTable/medications-table';

import {ClaimsProps} from './claims.types';
import {Button, TextField} from "@mui/material";
import {Label} from "@mui/icons-material";

export const Claims = React.forwardRef((props: ClaimsProps, ref: React.Ref<HTMLDivElement>) => {
  const {text, onClickLink} = props;
  return (
    <Grid container ref={ref}>
      <Grid item xs={12}>
        <Typography variant="h2">{text}-Search</Typography>
        <Label> field1 </Label><TextField ></TextField>
        <Label> field2 </Label><TextField></TextField>
        <Button onClick={onClickLink}> Search</Button>
        <MedicationsTable title="Claim Result" />
      </Grid>
    </Grid>
  );
});

export default Claims;
